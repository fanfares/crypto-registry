    "title": "Crypto Registry API",
    "description": "API for exchanges to submit their customer holdings, and for customers to verify their holdings",
    "version": "Version 1",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "schemas": {
      "FundingSubmissionStatus": {
        "type": "string",
        "enum": [
          "waiting-for-processing",
          "processing",
          "failed",
          "invalid-signatures",
          "cancelled",
          "accepted"
        ]
      },
      "Network": {
        "type": "string",
        "enum": [
          "testnet",
          "mainnet"
        ]
      },
      "FundingAddressStatus": {
        "type": "string",
        "enum": [
          "pending",
          "active",
          "cancelled"
        ]
      },
      "FundingAddressBase": {
        "type": "object",
        "properties": {
          "balance": {
            "type": "number"
          },
          "address": {
            "type": "string"
          },
          "signature": {
            "type": "string"
          },
          "message": {
            "type": "string"
          },
          "fundingSubmissionId": {
            "type": "string"
          },
          "validFromDate": {
            "format": "date-time",
            "type": "string"
          },
          "exchangeId": {
            "type": "string"
          },
          "network": {
            "$ref": "#/components/schemas/Network"
          },
          "status": {
            "$ref": "#/components/schemas/FundingAddressStatus"
          }
        },
        "required": [
          "address",
          "signature",
          "message",
          "fundingSubmissionId",
          "exchangeId",
          "network",
          "status"
        ]
      },
      "FundingSubmissionDto": {
        "type": "object",
        "properties": {
          "errorMessage": {
            "type": "string"
          },
          "network": {
            "type": "string"
          },
          "status": {
            "$ref": "#/components/schemas/FundingSubmissionStatus"
          },
          "exchangeId": {
            "type": "string"
          },
          "submissionFunds": {
            "type": "number"
          },
          "_id": {
            "type": "string"
          },
          "createdDate": {
            "format": "date-time",
            "type": "string"
          },
          "updatedDate": {
            "format": "date-time",
            "type": "string"
          },
          "addresses": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/FundingAddressBase"
            }
          }
        },
        "required": [
          "network",
          "status",
          "exchangeId",
          "_id",
          "createdDate",
          "updatedDate",
          "addresses"
        ]
      },
      "FundingDto": {
        "type": "object",
        "properties": {
          "current": {
            "$ref": "#/components/schemas/FundingSubmissionDto"
          },
          "pending": {
            "$ref": "#/components/schemas/FundingSubmissionDto"
          }
        },
        "required": [
          "current"
        ]
      },
      "CreateFundingAddressDto": {
        "type": "object",
        "properties": {
          "address": {
            "type": "string"
          },
          "signature": {
            "type": "string"
          },
          "message": {
            "type": "string"
          }
        },
        "required": [
          "address",
          "signature",
          "message"
        ]
      },
      "CreateFundingSubmissionDto": {
        "type": "object",
        "properties": {
          "addresses": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/CreateFundingAddressDto"
            }
          },
          "resetFunding": {
            "type": "boolean"
          }
        },
        "required": [
          "addresses",
          "resetFunding"
        ]
      },
      "SubmissionId": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          }
        },
        "required": [
          "id"
        ]
      },
      "CreateFundingSubmissionCsvDto": {
        "type": "object",
        "properties": {
          "resetFunding": {
            "type": "boolean"
          }
        },
        "required": [
          "resetFunding"
        ]
      },
      "HoldingsSubmissionDto": {
        "type": "object",
        "properties": {
          "totalHoldings": {
            "type": "number"
          },
          "exchangeId": {
            "type": "string"
          },
          "exchangeUid": {
            "type": "string"
          },
          "isCurrent": {
            "type": "boolean"
          },
          "_id": {
            "type": "string"
          },
          "createdDate": {
            "format": "date-time",
            "type": "string"
          },
          "updatedDate": {
            "format": "date-time",
            "type": "string"
          },
          "holdings": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        },
        "required": [
          "totalHoldings",
          "exchangeId",
          "isCurrent",
          "_id",
          "createdDate",
          "updatedDate",
          "holdings"
        ]
      },
      "CreateHoldingsSubmissionDto": {
        "type": "object",
        "properties": {
          "holdings": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        },
        "required": [
          "holdings"
        ]
      },
      "VerificationRequestDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string"
          }
        },
        "required": [
          "email"
        ]
      },
      "VerificationDto": {
        "type": "object",
        "properties": {
          "hashedEmail": {
            "type": "string"
          },
          "exchangeUid": {
            "type": "string"
          },
          "receivingAddress": {
            "type": "string"
          },
          "leaderAddress": {
            "type": "string"
          },
          "requestDate": {
            "format": "date-time",
            "type": "string"
          },
          "status": {
            "type": "string"
          }
        },
        "required": [
          "requestDate",
          "status"
        ]
      },
      "VerifyByUidDto": {
        "type": "object",
        "properties": {
          "uid": {
            "type": "string"
          }
        },
        "required": [
          "uid"
        ]
      },
      "VerifiedHoldingsDto": {
        "type": "object",
        "properties": {
          "holdingId": {
            "type": "string"
          },
          "fundingAsAt": {
            "format": "date-time",
            "type": "string"
          },
          "customerHoldingAmount": {
            "type": "number"
          },
          "exchangeName": {
            "type": "string"
          },
          "fundingSource": {
            "$ref": "#/components/schemas/Network"
          }
        },
        "required": [
          "holdingId",
          "fundingAsAt",
          "customerHoldingAmount",
          "exchangeName",
          "fundingSource"
        ]
      },
      "VerificationResultDto": {
        "type": "object",
        "properties": {
          "verificationId": {
            "type": "string"
          },
          "verifiedHoldings": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/VerifiedHoldingsDto"
            }
          }
        },
        "required": [
          "verificationId",
          "verifiedHoldings"
        ]
      }
    }
  }
