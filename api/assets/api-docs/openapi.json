  "info": {
    "title": "Crypto Registry API",
    "description": "API for exchanges to submit their customer holdings, and for customers to verify their holdings",
    "version": "Version 1",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "schemas": {
      "Network": {
        "type": "string",
        "enum": [
          "testnet",
          "mainnet"
        ]
      },
      "CustomerHoldingDto": {
        "type": "object",
        "properties": {
          "hashedEmail": {
            "type": "string"
          },
          "amount": {
            "type": "number"
          }
        },
        "required": [
          "hashedEmail",
          "amount"
        ]
      },
      "SubmissionWallet": {
        "type": "object",
        "properties": {
          "balance": {
            "type": "number"
          },
          "address": {
            "type": "string"
          },
          "signature": {
            "type": "string"
          }
        },
        "required": [
          "address"
        ]
      },
      "CreateSubmissionDto": {
        "type": "object",
        "properties": {
          "_id": {
            "type": "string"
          },
          "exchangeName": {
            "type": "string"
          },
          "network": {
            "$ref": "#/components/schemas/Network"
          },
          "receiverAddress": {
            "type": "string"
          },
          "leaderAddress": {
            "type": "string"
          },
          "customerHoldings": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/CustomerHoldingDto"
            }
          },
          "wallets": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/SubmissionWallet"
            }
          },
          "confirmationsRequired": {
            "type": "number"
          },
          "signingMessage": {
            "type": "string"
          }
        },
        "required": [
          "exchangeName",
          "network",
          "receiverAddress",
          "customerHoldings",
          "wallets",
          "signingMessage"
        ]
      },
      "SubmissionStatus": {
        "type": "string",
        "enum": [
          "new",
          "retrieving-wallet-balance",
          "insufficient-funds",
          "cancelled",
          "waiting-for-confirmation",
          "confirmed",
          "rejected",
          "processing-failed",
          "invalid-signature"
        ]
      },
      "SubmissionConfirmationBase": {
        "type": "object",
        "properties": {}
      },
      "SubmissionDto": {
        "type": "object",
        "properties": {
          "receiverAddress": {
            "type": "string"
          },
          "errorMessage": {
            "type": "string"
          },
          "network": {
            "type": "string"
          },
          "status": {
            "$ref": "#/components/schemas/SubmissionStatus"
          },
          "totalCustomerFunds": {
            "type": "number"
          },
          "totalExchangeFunds": {
            "type": "number"
          },
          "exchangeName": {
            "type": "string"
          },
          "wallets": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/SubmissionWallet"
            }
          },
          "signingMessage": {
            "type": "string"
          },
          "isCurrent": {
            "type": "boolean"
          },
          "confirmationsRequired": {
            "type": "number"
          },
          "confirmationDate": {
            "format": "date-time",
            "type": "string"
          },
          "_id": {
            "type": "string"
          },
          "confirmations": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/SubmissionConfirmationBase"
            }
          }
        },
        "required": [
          "receiverAddress",
          "network",
          "status",
          "totalCustomerFunds",
          "totalExchangeFunds",
          "exchangeName",
          "wallets",
          "signingMessage",
          "isCurrent",
          "confirmationsRequired",
          "confirmationDate",
          "_id",
          "confirmations"
        ]
      },
      "SubmissionId": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          }
        },
        "required": [
          "id"
        ]
      },
      "CreateSubmissionCsvDto": {
        "type": "object",
        "properties": {
          "exchangeName": {
            "type": "string"
          },
          "network": {
            "$ref": "#/components/schemas/Network"
          }
        },
        "required": [
          "exchangeName",
          "network"
        ]
      },
      "VerificationRequestDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string"
          }
        },
        "required": [
          "email"
        ]
      },
      "VerificationDto": {
        "type": "object",
        "properties": {
          "hashedEmail": {
            "type": "string"
          },
          "receivingAddress": {
            "type": "string"
          },
          "leaderAddress": {
            "type": "string"
          },
          "requestDate": {
            "format": "date-time",
            "type": "string"
          },
          "status": {
            "type": "string"
          }
        },
        "required": [
          "hashedEmail",
          "receivingAddress",
          "requestDate",
          "status"
        ]
      }
    }
  }
}