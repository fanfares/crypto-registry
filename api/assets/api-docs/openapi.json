  "components": {
    "schemas": {
      "ExchangeDto": {
        "type": "object",
        "properties": {
          "_id": {
            "type": "string"
          },
          "exchangeName": {
            "type": "string"
          }
        },
        "required": [
          "_id",
          "exchangeName"
        ]
      },
      "CustomerHoldingDto": {
        "type": "object",
        "properties": {
          "hashedEmail": {
            "type": "string"
          },
          "amount": {
            "type": "number"
          }
        },
        "required": [
          "hashedEmail",
          "amount"
        ]
      },
      "SubmissionDto": {
        "type": "object",
        "properties": {
          "exchangeName": {
            "type": "string"
          },
          "customerHoldings": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/CustomerHoldingDto"
            }
          }
        },
        "required": [
          "exchangeName",
          "customerHoldings"
        ]
      },
      "SubmissionStatus": {
        "type": "string",
        "enum": [
          "unused",
          "waiting-for-payment",
          "complete"
        ]
      },
      "SubmissionStatusDto": {
        "type": "object",
        "properties": {
          "paymentAddress": {
            "type": "string"
          },
          "paymentAmount": {
            "type": "number"
          },
          "submissionStatus": {
            "$ref": "#/components/schemas/SubmissionStatus"
          }
        },
        "required": [
          "paymentAddress",
          "paymentAmount",
          "submissionStatus"
        ]
      },
      "EmailDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string"
          }
        },
        "required": [
          "email"
        ]
      },
      "VerificationResult": {
        "type": "string",
        "enum": [
          "email-sent",
          "failed-to-send-email",
          "cant-find-holdings-for-email",
          "cant-find-verified-holdings-for-email"
        ]
      },
      "VerificationDto": {
        "type": "object",
        "properties": {
          "verificationResult": {
            "$ref": "#/components/schemas/VerificationResult"
          }
        },
        "required": [
          "verificationResult"
        ]
      },
      "SystemConfig": {
        "type": "object",
        "properties": {
          "registryKey": {
            "type": "string"
          },
          "docsUrl": {
            "type": "string"
          },
          "apiUrl": {
            "type": "string"
          }
        },
        "required": [
          "registryKey",
          "docsUrl",
          "apiUrl"
        ]
      },
      "SystemStatus": {
        "type": "object",
        "properties": {
          "status": {
            "type": "string"
          }
        },
        "required": [
          "status"
        ]
      },
      "SendTestEmailDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string"
          }
        },
        "required": [
          "email"
        ]
      }
    }
  }
