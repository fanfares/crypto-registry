{
  "openapi": "3.0.0",
  "paths": {
    "/submission": {
      "post": {
        "operationId": "SubmissionController_createSubmission",
        "parameters": [],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateSubmissionDto"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": ""
          }
        },
        "tags": [
          "submission"
        ]
      }
    },
    "/submission/cancel": {
      "post": {
        "operationId": "SubmissionController_cancelSubmission",
        "parameters": [],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/AddressDto"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": ""
          }
        },
        "tags": [
          "submission"
        ]
      }
    },
    "/submission/{address}": {
      "get": {
        "operationId": "SubmissionController_getSubmissionStatus",
        "parameters": [
          {
            "name": "address",
            "required": true,
            "in": "path",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "default": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/SubmissionStatusDto"
                }
              }
            }
          }
        },
        "tags": [
          "submission"
        ]
      }
    },
    "/submission/submit-csv": {
      "post": {
        "operationId": "SubmissionController_submitCustomersHoldingsCsv",
        "parameters": [],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateSubmissionCsvDto"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": ""
          }
        },
        "tags": [
          "submission"
        ]
      }
    },
    "/customer/verify": {
      "post": {
        "operationId": "CustomerController_verifyHoldings",
        "parameters": [],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/EmailDto"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": ""
          }
        },
        "tags": [
          "customer"
        ]
      }
    }
  },
  "info": {
    "title": "Crypto Registry API",
    "description": "API for exchanges to submit their customer holdings, and for customers to verify their holdings",
    "version": "Version 1",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "schemas": {
      "CustomerHoldingDto": {
        "type": "object",
        "properties": {
          "hashedEmail": {
            "type": "string"
          },
          "amount": {
            "type": "number"
          }
        },
        "required": [
          "hashedEmail",
          "amount"
        ]
      },
      "CreateSubmissionDto": {
        "type": "object",
        "properties": {
          "exchangeZpub": {
            "type": "string"
          },
          "exchangeName": {
            "type": "string"
          },
          "customerHoldings": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/CustomerHoldingDto"
            }
          }
        },
        "required": [
          "exchangeZpub",
          "exchangeName",
          "customerHoldings"
        ]
      },
      "AddressDto": {
        "type": "object",
        "properties": {
          "address": {
            "type": "string"
          }
        },
        "required": [
          "address"
        ]
      },
      "SubmissionStatus": {
        "type": "string",
        "enum": [
          "waiting-for-payment",
          "cancelled",
          "insufficient-funds",
          "sender-mismatch",
          "verified"
        ]
      },
      "SubmissionStatusDto": {
        "type": "object",
        "properties": {
          "paymentAddress": {
            "type": "string"
          },
          "totalCustomerFunds": {
            "type": "number"
          },
          "totalExchangeFunds": {
            "type": "number"
          },
          "paymentAmount": {
            "type": "number"
          },
          "exchangeName": {
            "type": "string"
          },
          "isCurrent": {
            "type": "boolean"
          },
          "status": {
            "$ref": "#/components/schemas/SubmissionStatus"
          }
        },
        "required": [
          "paymentAddress",
          "paymentAmount",
          "exchangeName",
          "isCurrent",
          "status"
        ]
      },
      "CreateSubmissionCsvDto": {
        "type": "object",
        "properties": {
          "exchangeZpub": {
            "type": "string"
          },
          "exchangeName": {
            "type": "string"
          }
        },
        "required": [
          "exchangeZpub",
          "exchangeName"
        ]
      },
      "EmailDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string"
          }
        },
        "required": [
          "email"
        ]
      }
    }
  }
}