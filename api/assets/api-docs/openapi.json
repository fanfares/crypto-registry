  "components": {
    "schemas": {
      "CustomerHoldingDto": {
        "type": "object",
        "properties": {
          "hashedEmail": {
            "type": "string"
          },
          "amount": {
            "type": "number"
          }
        },
        "required": [
          "hashedEmail",
          "amount"
        ]
      },
      "CreateSubmissionDto": {
        "type": "object",
        "properties": {
          "exchangeZpub": {
            "type": "string"
          },
          "exchangeName": {
            "type": "string"
          },
          "customerHoldings": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/CustomerHoldingDto"
            }
          }
        },
        "required": [
          "exchangeZpub",
          "exchangeName",
          "customerHoldings"
        ]
      },
      "AddressDto": {
        "type": "object",
        "properties": {
          "address": {
            "type": "string"
          }
        },
        "required": [
          "address"
        ]
      },
      "SubmissionStatus": {
        "type": "string",
        "enum": [
          "waiting-for-payment",
          "cancelled",
          "insufficient-funds",
          "sender-mismatch",
          "verified"
        ]
      },
      "SubmissionStatusDto": {
        "type": "object",
        "properties": {
          "paymentAddress": {
            "type": "string"
          },
          "totalCustomerFunds": {
            "type": "number"
          },
          "totalExchangeFunds": {
            "type": "number"
          },
          "paymentAmount": {
            "type": "number"
          },
          "exchangeName": {
            "type": "string"
          },
          "status": {
            "$ref": "#/components/schemas/SubmissionStatus"
          }
        },
        "required": [
          "paymentAddress",
          "paymentAmount",
          "exchangeName",
          "status"
        ]
      },
      "CreateSubmissionCsvDto": {
        "type": "object",
        "properties": {
          "exchangeZpub": {
            "type": "string"
          },
          "exchangeName": {
            "type": "string"
          }
        },
        "required": [
          "exchangeZpub",
          "exchangeName"
        ]
      },
      "EmailDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string"
          }
        },
        "required": [
          "email"
        ]
      },
      "IsValid": {
        "type": "object",
        "properties": {
          "isValid": {
            "type": "boolean"
          }
        },
        "required": [
          "isValid"
        ]
      },
      "TransactionInput": {
        "type": "object",
        "properties": {
          "txid": {
            "type": "string"
          },
          "address": {
            "type": "string"
          },
          "value": {
            "type": "number"
          }
        },
        "required": [
          "txid",
          "address",
          "value"
        ]
      },
      "TransactionOutput": {
        "type": "object",
        "properties": {
          "address": {
            "type": "string"
          },
          "value": {
            "type": "number"
          }
        },
        "required": [
          "address",
          "value"
        ]
      },
      "Transaction": {
        "type": "object",
        "properties": {
          "txid": {
            "type": "string"
          },
          "fee": {
            "type": "number"
          },
          "blockTime": {
            "format": "date-time",
            "type": "string"
          },
          "inputValue": {
            "type": "number"
          },
          "inputs": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/TransactionInput"
            }
          },
          "outputs": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/TransactionOutput"
            }
          }
        },
        "required": [
          "txid",
          "fee",
          "blockTime",
          "inputValue",
          "inputs",
          "outputs"
        ]
      },
      "SystemConfig": {
        "type": "object",
        "properties": {
          "docsUrl": {
            "type": "string"
          }
        },
        "required": [
          "docsUrl"
        ]
      },
      "SystemStatus": {
        "type": "object",
        "properties": {
          "status": {
            "type": "string"
          }
        },
        "required": [
          "status"
        ]
      },
      "SendTestEmailDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string"
          }
        },
        "required": [
          "email"
        ]
      },
      "SendFundsDto": {
        "type": "object",
        "properties": {
          "senderZpub": {
            "type": "string"
          },
          "toAddress": {
            "type": "string"
          },
          "amount": {
            "type": "number"
          }
        },
        "required": [
          "senderZpub",
          "toAddress",
          "amount"
        ]
      },
      "ExchangeDto": {
        "type": "object",
        "properties": {
          "_id": {
            "type": "string"
          },
          "exchangeName": {
            "type": "string"
          }
        },
        "required": [
          "_id",
          "exchangeName"
        ]
      }
    }
  }
}