  "components": {
    "schemas": {
      "ExchangeDto": {
        "type": "object",
        "properties": {
          "_id": {
            "type": "string"
          },
          "custodianName": {
            "type": "string"
          },
          "publicKey": {
            "type": "string"
          },
          "isRegistered": {
            "type": "boolean"
          }
        },
        "required": [
          "_id",
          "custodianName",
          "publicKey",
          "isRegistered"
        ]
      },
      "CustomerHolding": {
        "type": "object",
        "properties": {
          "publicKey": {
            "type": "string"
          },
          "hashedEmail": {
            "type": "string"
          },
          "amount": {
            "type": "number"
          }
        },
        "required": [
          "publicKey",
          "hashedEmail",
          "amount"
        ]
      },
      "CustomerHoldingsDto": {
        "type": "object",
        "properties": {
          "customerHoldings": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/CustomerHolding"
            }
          }
        },
        "required": [
          "customerHoldings"
        ]
      },
      "RegistrationCheckResult": {
        "type": "object",
        "properties": {
          "isRegistered": {
            "type": "boolean"
          },
          "isPaymentMade": {
            "type": "boolean"
          }
        },
        "required": [
          "isRegistered",
          "isPaymentMade"
        ]
      },
      "EmailDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string"
          }
        },
        "required": [
          "email"
        ]
      },
      "VerificationResult": {
        "type": "string",
        "enum": [
          "email-sent",
          "failed-to-send-email",
          "cant-find-holdings-for-email",
          "cant-find-verified-holdings-for-email"
        ]
      },
      "VerificationDto": {
        "type": "object",
        "properties": {
          "verificationResult": {
            "$ref": "#/components/schemas/VerificationResult"
          }
        },
        "required": [
          "verificationResult"
        ]
      },
      "SendTestEmailDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string"
          }
        },
        "required": [
          "email"
        ]
      },
      "SystemConfig": {
        "type": "object",
        "properties": {
          "registryKey": {
            "type": "string"
          },
          "docsUrl": {
            "type": "string"
          },
          "apiUrl": {
            "type": "string"
          }
        },
        "required": [
          "registryKey",
          "docsUrl",
          "apiUrl"
        ]
      },
      "SystemStatus": {
        "type": "object",
        "properties": {
          "status": {
            "type": "string"
          }
        },
        "required": [
          "status"
        ]
      }
