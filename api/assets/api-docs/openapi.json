  "components": {
    "schemas": {
      "AddressSubmissionStatus": {
        "type": "string",
        "enum": [
          "new",
          "retrieving-balances",
          "failed",
          "invalid-signatures",
          "cancelled",
          "ok"
        ]
      },
      "RegisteredAddress": {
        "type": "object",
        "properties": {
          "balance": {
            "type": "number"
          },
          "address": {
            "type": "string"
          },
          "signature": {
            "type": "string"
          }
        },
        "required": [
          "balance",
          "address",
          "signature"
        ]
      },
      "AddressSubmissionDto": {
        "type": "object",
        "properties": {
          "errorMessage": {
            "type": "string"
          },
          "network": {
            "type": "string"
          },
          "status": {
            "$ref": "#/components/schemas/AddressSubmissionStatus"
          },
          "exchangeId": {
            "type": "string"
          },
          "addresses": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/RegisteredAddress"
            }
          },
          "totalFunds": {
            "type": "number"
          },
          "signingMessage": {
            "type": "string"
          },
          "isCurrent": {
            "type": "boolean"
          },
          "_id": {
            "type": "string"
          },
          "createdDate": {
            "format": "date-time",
            "type": "string"
          },
          "updatedDate": {
            "format": "date-time",
            "type": "string"
          }
        },
        "required": [
          "network",
          "status",
          "exchangeId",
          "addresses",
          "totalFunds",
          "signingMessage",
          "isCurrent",
          "_id",
          "createdDate",
          "updatedDate"
        ]
      },
      "Network": {
        "type": "string",
        "enum": [
          "testnet",
          "mainnet"
        ]
      },
      "CreateRegisteredAddressDto": {
        "type": "object",
        "properties": {
          "address": {
            "type": "string"
          },
          "signature": {
            "type": "string"
          }
        },
        "required": [
          "address",
          "signature"
        ]
      },
      "CreateAddressSubmissionDto": {
        "type": "object",
        "properties": {
          "exchangeId": {
            "type": "string"
          },
          "network": {
            "$ref": "#/components/schemas/Network"
          },
          "addresses": {
            "type": "array",
