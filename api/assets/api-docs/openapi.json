              "$ref": "#/components/schemas/CreateRegisteredAddressDto"
            }
          },
          "signingMessage": {
            "type": "string"
          }
        },
        "required": [
          "exchangeId",
          "network",
          "addresses",
          "signingMessage"
        ]
      },
      "SubmissionId": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          }
        },
        "required": [
          "id"
        ]
      },
      "HoldingsSubmissionDto": {
        "type": "object",
        "properties": {
          "totalCustomerFunds": {
            "type": "number"
          },
          "exchangeId": {
            "type": "string"
          },
          "isCurrent": {
            "type": "boolean"
          },
          "network": {
            "$ref": "#/components/schemas/Network"
          },
          "_id": {
            "type": "string"
          },
          "createdDate": {
            "format": "date-time",
            "type": "string"
          },
          "updatedDate": {
            "format": "date-time",
            "type": "string"
          },
          "holdings": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        },
        "required": [
          "totalCustomerFunds",
          "exchangeId",
          "isCurrent",
          "network",
          "_id",
          "createdDate",
          "updatedDate",
          "holdings"
        ]
      },
      "CreateHoldingsSubmissionDto": {
        "type": "object",
        "properties": {
          "network": {
            "$ref": "#/components/schemas/Network"
          },
          "exchangeId": {
            "type": "string"
          },
          "holdings": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        },
        "required": [
          "network",
          "exchangeId",
          "holdings"
        ]
      },
      "CreateHoldingSubmissionCsvDto": {
        "type": "object",
        "properties": {
          "network": {
            "$ref": "#/components/schemas/Network"
          },
          "exchangeId": {
            "type": "string"
          }
        },
        "required": [
          "network",
          "exchangeId"
        ]
      },
      "VerificationRequestDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string"
          }
        },
        "required": [
          "email"
        ]
      },
      "VerificationDto": {
        "type": "object",
        "properties": {
          "hashedEmail": {
            "type": "string"
          },
          "receivingAddress": {
            "type": "string"
          },
          "leaderAddress": {
            "type": "string"
          },
          "requestDate": {
            "format": "date-time",
            "type": "string"
          },
          "status": {
            "type": "string"
          }
        },
        "required": [
          "hashedEmail",
          "receivingAddress",
          "requestDate",
          "status"
        ]
      }
    }
