{
  "openapi": "3.0.0",
  "paths": {
    "/api/custodian/submit-holdings": {
      "post": {
        "operationId": "CustodianController_submitCustodianHoldings",
        "parameters": [],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CustomerHoldingsDto"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": ""
          }
        },
        "tags": [
          "custodian"
        ]
      }
    },
    "/api/customer/verify-holdings": {
      "post": {
        "operationId": "CustomerController_verifyHoldings",
        "parameters": [],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/EmailDto"
              }
            }
          }
        },
        "responses": {
          "default": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/VerificationDto"
                }
              }
            }
          }
        },
        "tags": [
          "customer"
        ]
      }
    },
    "/api/block-chain/get-balance/{publicKey}": {
      "get": {
        "operationId": "BlockChainController_getBalance",
        "parameters": [
          {
            "name": "publicKey",
            "required": true,
            "in": "path",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": ""
          }
        },
        "tags": [
          "block-chain"
        ]
      }
    },
    "/api/system/test": {
      "get": {
        "operationId": "SystemController_systemTest",
        "parameters": [],
        "responses": {
          "default": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/SystemStatus"
                }
              }
            }
          }
        },
        "tags": [
          "system"
        ]
      }
    },
    "/api/system/send-test-email": {
      "post": {
        "operationId": "SystemController_sendTestEmail",
        "parameters": [],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/SendTestEmailDto"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": ""
          }
        },
        "tags": [
          "system"
        ]
      }
    }
  },
  "info": {
    "title": "Custodian Registry API",
    "description": "TBC",
    "version": "TCB",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "schemas": {
      "CustomerHolding": {
        "type": "object",
        "properties": {
          "hashedEmail": {
            "type": "string"
          },
          "amount": {
            "type": "number"
          }
        },
        "required": [
          "hashedEmail",
          "amount"
        ]
      },
      "CustomerHoldingsDto": {
        "type": "object",
        "properties": {
          "custodianName": {
            "type": "string"
          },
          "publicKey": {
            "type": "string"
          },
          "customerHoldings": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/CustomerHolding"
            }
          }
        },
        "required": [
          "custodianName",
          "publicKey",
          "customerHoldings"
        ]
      },
      "EmailDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string"
          }
        },
        "required": [
          "email"
        ]
      },
      "VerificationResult": {
        "type": "string",
        "enum": [
          "email-sent",
          "failed-to-send-email",
          "cant-find-verified-holding"
        ]
      },
      "VerificationDto": {
        "type": "object",
        "properties": {
          "verificationResult": {
            "$ref": "#/components/schemas/VerificationResult"
          }
        },
        "required": [
          "verificationResult"
        ]
      },
      "SystemStatus": {
        "type": "object",
        "properties": {
          "status": {
            "type": "string"
          }
        },
        "required": [
          "status"
        ]
      },
      "SendTestEmailDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string"
          }
        },
        "required": [
          "email"
        ]
      }
    }
  }
}